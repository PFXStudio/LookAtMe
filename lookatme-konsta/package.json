{
  "name": "konsta",
  "description": "Mobile UI components made with Tailwind CSS",
  "version": "1.0.2",
  "private": true,
  "license": "MIT",
  "author": "Vladimir Kharlampidi",
  "scripts": {
    "/*============ Svelte Kitchen Sink ============*/": "============",
    "svelte": "cross-env NODE_ENV=development vite ./kitchen-sink/svelte",
    "build-svelte-ks": "cross-env NODE_ENV=production vite build ./kitchen-sink/svelte",
    "/*============ Dev tools ============*/": "============",
    "prettier": "prettier \"**/*.+(js|json|scss|css|less|ts|vue|svelte)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ext .js,.vue,.jsx,.svelte .",
    "validate": "npm-run-all --parallel check-format lint",
    "release": "npm run validate && node ./scripts/release",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -u -s",
    "build-sponsors": "node ./scripts/build-sponsors.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@material/material-color-utilities": "^0.2.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "autoprefixer": "^10.4.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-svelte3": "^3.4.1",
    "exec-sh": "^0.4.0",
    "framework7-icons": "^5.0.5",
    "fs-extra": "^10.0.1",
    "history": "^5.3.0",
    "inquirer": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.77.2",
    "svelte": "^3.50.0",
    "svelte-navigator": "^3.1.5",
    "tailwindcss": "^3.1.8",
    "vite": "^3.1.0",
    "qs": "^6.11.0"
  },
  "dependencies": {
    "konsta": "file:src",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-spa-router": "^3.3.0"
  }
}
